{
  "name": "@zd~/request",
  "version": "0.5.0",
  "private": false,
  "description": "A request library.",
  "author": "zd",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhuddan/request.git"
  },
  "keywords": [
    "request",
    "axios",
    "qs",
    "uni-app",
    "weixin",
    "miniprogram",
    "http"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs.js"
      }
    },
    "./*": "./*",
    "./http": {
      "import": {
        "types": "./dist/http.d.ts",
        "default": "./dist/http.esm.js"
      },
      "require": {
        "types": "./dist/http.d.ts",
        "default": "./dist/http.cjs.js"
      }
    },
    "./wx": {
      "import": {
        "types": "./dist/wx.d.ts",
        "default": "./dist/wx.esm.js"
      },
      "require": {
        "types": "./dist/wx.d.ts",
        "default": "./dist/wx.cjs.js"
      }
    },
    "./uni": {
      "import": {
        "types": "./dist/uni.d.ts",
        "default": "./dist/uni.esm.js"
      },
      "require": {
        "types": "./dist/uni.d.ts",
        "default": "./dist/uni.cjs.js"
      }
    },
    "./taro": {
      "import": {
        "types": "./dist/taro.d.ts",
        "default": "./dist/taro.esm.js"
      },
      "require": {
        "types": "./dist/taro.d.ts",
        "default": "./dist/taro.cjs.js"
      }
    }
  },
  "main": "index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist/*",
    "index.js",
    "scripts/*"
  ],
  "scripts": {
    "bumpp": "bumpp"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "lodash.merge": "^4.6.2",
    "miniprogram-api-typings": "^3.12.3",
    "qs": "^6.13.0"
  },
  "devDependencies": {
    "@tarojs/taro": "^4.0.7",
    "@types/lodash.merge": "^4.6.9",
    "@types/qs": "^6.9.15"
  }
}
