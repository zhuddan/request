{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "types": [
      "miniprogram-api-typings",
      "@dcloudio/types"
    ],
    "strict": true,
    "declaration": true,
    "declarationMap": false,
    "outDir": "dist",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src", "dist"]
}
