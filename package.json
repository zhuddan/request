{
  "scripts": {
    "dev": "rollup -c rollup.config.dev.mjs -w",
    "example:uni": "pnpm -F uni-example dev",
    "example:vue": "pnpm -F vue-example dev",
    "build": "rollup -c rollup.config.build.mjs",
    "publish": "node ./scripts/publish.mjs",
    "bumpp": "pnpm run bumpp -- patch --packages=packages/your-sub-package",
