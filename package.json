    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/lodash-es": "^4.17.12",
    "@vue/compiler-sfc": "^3.4.38",
    "bumpp": "^9.5.2",
    "cross-env": "^7.0.3",
    "eslint": "9.5.0",
    "eslint-plugin-format": "^0.1.2",
    "picocolors": "^1.1.0",
    "rollup": "^4.21.2",
    "rollup-plugin-delete": "^2.0.0",
