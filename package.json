{
  "name": "@zd~/request",
  "version": "0.0.7",
  "private": false,
  "description": "A request library based on Axios.",
  "author": "zd",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhuddan/http-request.git"
  },
  "keywords": [
    "request"
  ],
  "exports": {
    "./request": {
      "types": "./dist/request.d.ts",
      "import": "./dist/request.js",
      "require": "./dist/request.cjs"
    },
    "./uni": {
      "types": "./dist/uni.d.ts",
      "import": "./dist/uni.js",
      "require": "./dist/uni.cjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "release": "bumpp"
  },
  "dependencies": {
    "@dcloudio/types": "^3.4.12",
    "axios": "^1.7.3",
    "lodash-es": "^4.17.21",
    "qs": "^6.13.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.24.1",
    "@types/lodash-es": "^4.17.12",
    "@types/qs": "^6.9.15",
    "bumpp": "^9.4.2",
    "eslint": "9.5.0",
    "eslint-plugin-format": "^0.1.2",
    "terser": "^5.31.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
}
