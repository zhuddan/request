{
  "name": "@zd~/request",
  "version": "0.0.49",
  "private": false,
  "description": "A request library based on Axios.",
  "author": "zd",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhuddan/request.git"
  },
  "keywords": [
    "request"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": "./*",
    "./shared": {
      "import": {
        "types": "./dist/shared.d.ts",
        "default": "./dist/shared.mjs"
      },
      "require": {
        "types": "./dist/shared.d.ts",
        "default": "./dist/shared.js"
      }
    },
    "./http": {
      "import": {
        "types": "./dist/http.d.ts",
        "default": "./dist/http.mjs"
      },
      "require": {
        "types": "./dist/http.d.ts",
        "default": "./dist/http.js"
      }
    },
    "./uni": {
      "import": {
        "types": "./dist/uni.d.ts",
        "default": "./dist/uni.mjs"
      },
      "require": {
        "types": "./dist/uni.d.ts",
        "default": "./dist/uni.js"
      }
    },
    "./wx": {
      "import": {
        "types": "./dist/wx.d.ts",
        "default": "./dist/wx.mjs"
      },
      "require": {
        "types": "./dist/wx.d.ts",
        "default": "./dist/wx.js"
      }
    }
  },
  "main": "index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist/*",
    "index.js",
    "scripts/*"
  ],
  "scripts": {
    "build:tsup": "tsup",
    "dev": "cross-env NODE_ENV=development rollup -c -w",
    "build": "tsc -p tsconfig.build.json && cross-env NODE_ENV=production rollup -c",
    "publish": "node ./scripts/publish.mjs",
    "release": "bumpp"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "qs": "^6.13.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.24.1",
    "@dcloudio/types": "^3.4.12",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/qs": "^6.9.15",
    "@vue/compiler-sfc": "^3.4.37",
    "bumpp": "^9.4.2",
    "cross-env": "^7.0.3",
    "eslint": "9.5.0",
    "eslint-plugin-format": "^0.1.2",
    "picocolors": "^1.0.1",
    "rollup": "^4.20.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.5.4"
  }
}
