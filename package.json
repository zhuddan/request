{
  "name": "@zd~/request",
  "version": "0.0.20",
  "private": false,
  "description": "A request library based on Axios.",
  "author": "zd",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhuddan/http-request.git"
  },
  "keywords": [
    "request"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./*": "./*",
    "./shared": {
      "import": {
        "types": "./dist/shared.d.mts",
        "default": "./dist/shared.js"
      },
      "require": {
        "types": "./dist/shared.d.ts",
        "default": "./dist/shared.cjs"
      }
    },
    "./request": {
      "import": {
        "types": "./dist/request.d.mts",
        "default": "./dist/request.js"
      },
      "require": {
        "types": "./dist/request.d.ts",
        "default": "./dist/request.cjs"
      }
    },
    "./uni": {
      "import": {
        "types": "./dist/uni.d.mts",
        "default": "./dist/uni.js"
      },
      "require": {
        "types": "./dist/uni.d.ts",
        "default": "./dist/uni.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "release": "bumpp"
  },
  "peerDependencies": {
    "axios": "^1.7.3"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "lodash-es": "^4.17.21",
    "qs": "^6.13.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.24.1",
    "@dcloudio/types": "^3.4.12",
    "@types/lodash-es": "^4.17.12",
    "@types/qs": "^6.9.15",
    "bumpp": "^9.4.2",
    "eslint": "9.5.0",
    "eslint-plugin-format": "^0.1.2",
    "terser": "^5.31.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
}
